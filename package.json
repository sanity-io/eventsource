{
  "name": "@sanity/eventsource",
  "version": "6.0.0-canary.2",
  "description": "EventSource polyfill for browser and node.js",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "eventsource",
    "polyfill"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/eventsource.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/node.ts",
      "browser": {
        "source": "./src/browser.ts",
        "import": "./dist/browser.mjs",
        "require": "./dist/browser.js"
      },
      "react-native": {
        "import": "./dist/browser.mjs",
        "default": "./dist/browser.js"
      },
      "deno": "./dist/browser.mjs",
      "edge-light": "./dist/browser.mjs",
      "worker": "./dist/browser.mjs",
      "node": {
        "import": "./dist/node.mjs",
        "default": "./dist/node.js"
      },
      "import": "./dist/node.mjs",
      "default": "./dist/node.js"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "default": "./dist/browser.js"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "default": "./dist/node.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/node.js",
  "browser": {
    "./dist/node.js": "./dist/browser.js",
    "./dist/node.mjs": "./dist/browser.mjs"
  },
  "types": "./dist/node.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/browser.d.ts"
      ],
      "node": [
        "./dist/node.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "browser.js",
    "node.js"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --clean --check",
    "prepublishOnly": "npm run build",
    "test": "tsc"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": "@sanity/prettier-config",
  "dependencies": {
    "@types/eventsource": "1.1.15",
    "eventsource": "2.0.2"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.8.6",
    "@sanity/prettier-config": "^1.0.2",
    "prettier": "^3.2.5",
    "typescript": "5.4.5"
  }
}
